// simple random number generator suggested by c standard
int weird(int a) 
{
    a = a * 1103515245 + 12345;
    return ((a/65536) % 32768);
}

void main(){
    int i, j, k, l;
    int acc;
    acc = 0;

lb1:
    for(i=0; i<10; i++) {
        acc += i;
lbl2:   for(j=0; j<20; j++) {
            acc += j;
lbl3:       for(k=0; k<100; k++) {
                acc += k;
                if(weird(acc) % 47 == 0) break lbl2;
lbl4:           for(l=0; l<200; l++) {
                    acc += l;
                    if(weird(acc) % 33 == 0) continue lbl3;
                    if(weird(acc++) % (i+j-k+l) == 0) break lbl1;
                    acc--;
                }
            }
            
            if(weird(acc) % (i+j) == 0) continue lbl1;
        }
    }
}
