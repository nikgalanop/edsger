#include "stdio.h"

void main() {
	int a;
	int *A;
	char c;
	char *C;
	bool b;
	bool *B;
	double d;
	double *D;
	
	a = 80;
	A = &a;
	c = 'f';
	C = &c;
	b = false;
	B = &b;
	d = 3.14159;
	D = &d;
	
	writeString("All to int\n");
	writeInteger(a);
	writeChar('\n');
	writeInteger((int)A);
	writeChar('\n');
	writeString("Reading from int*: ");
	writeInteger(*(int*)A);
	writeChar('\n');
	writeInteger((int)c);
	writeChar('\n');
	writeInteger((int)C);
	writeChar('\n');
	writeString("Reading from char*: ");
	writeInteger(*(int*)C);
	writeChar('\n');
	writeInteger((int)b);
	writeChar('\n');
	writeInteger((int)B);
	writeChar('\n');
	writeString("Reading from bool*: ");
	writeInteger(*(int*)B);
	writeChar('\n');
	writeInteger((int)d);
	writeChar('\n');
	writeInteger((int)D);
	writeChar('\n');
	writeString("Reading from double*: ");
	writeInteger(*(int*)D);
	writeChar('\n');
	writeChar('\n');

	writeString("All to double\n");
	writeReal((double)a);
	writeChar('\n');
	writeString("Reading from int*: ");
	writeReal(*(double*)A);
	writeChar('\n');
	writeReal((double)b);
	writeChar('\n');
	writeString("Reading from bool*: ");
	writeReal(*(double*)B);
	writeChar('\n');
	writeReal((double)c);
	writeChar('\n');
	writeString("Reading from char*: ");
	writeReal(*(double*)C);
	writeChar('\n');
	writeReal((double)d);
	writeChar('\n');
	writeString("Reading from double*: ");
	writeReal(*(double*)D);
	writeChar('\n');
	writeChar('\n');

	writeString("All to bool\n");
	writeBoolean((bool)a);
	writeChar('\n');
	writeString("Reading from int*: ");
	writeBoolean(*(bool*)A);
	writeChar('\n');
	writeBoolean(b);
	writeChar('\n');
	writeString("Reading from bool*: ");
	writeBoolean(*(bool*)B);
	writeChar('\n');
	writeBoolean((bool)c);
	writeChar('\n');
	writeString("Reading from char*: ");
	writeBoolean(*(bool*)C);
	writeChar('\n');
	writeBoolean((bool)d);
	writeChar('\n');
	writeString("Reading from double*: ");
	writeBoolean(*(bool*)D);
	writeChar('\n');
	
	writeString("All to char\n");
	writeChar((char)a);
	writeChar('\n');
	writeString("Reading from int*: ");
	writeChar(*(char*)A);
	writeChar('\n');
	writeChar((char)b);
	writeChar('\n');
	writeString("Reading from bool*: ");
	writeChar(*(char*)B);
	writeChar('\n');
	writeChar(c);
	writeChar('\n');
	writeString("Reading from char*: ");
	writeChar(*(char*)C);
	writeChar('\n');
	writeChar((char) d);
	writeChar('\n');
	writeString("Reading from double*: ");
	writeChar(*(char*)D);
	writeChar('\n');
}
